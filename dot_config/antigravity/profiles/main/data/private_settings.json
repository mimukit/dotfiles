    "wrangler.json": "jsonc",
  },
  "explorer.compactFolders": false,
  "tailwindCSS.includeLanguages": {
    "typescriptreact": "javascript",
    "typescript": "javascript",
  },
  "tailwindCSS.emmetCompletions": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "explorer.confirmDragAndDrop": false,
  "editor.fontFamily": "'FiraMono Nerd Font', Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.cursorStyleInactive": "block",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "diffEditor.renderSideBySide": true,
  "vim.enableNeovim": false,
  "vim.neovimUseConfigFile": false,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.camelCaseMotion.enable": true,
  "vim.surround": true,
  "vim.foldfix": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindings": [
    {
      "before": ["tab"],
      "commands": ["editor.action.indentLines"],
    },
    {
      "before": ["<S-tab>"],
      "commands": ["editor.action.outdentLines"],
    },
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    // Insert empty line
    {
      "before": ["g", "o"],
      "commands": ["editor.action.insertLineAfter"],
    },
    {
      "before": ["g", "O"],
      "commands": ["editor.action.insertLineBefore"],
    },
    // Easy motion trigger
    {
      "before": ["s"],
      "after": ["<leader>", "<leader>", "2", "s"],
    },
    // Navigation
    {
      "before": ["<S-h>"],
      "commands": [":bprevious"],
    },
    {
      "before": ["S-l>"],
      "commands": [":bnext"],
    },
    // Splits
    {
      "before": ["leader", "v"],
      "commands": [":vsplit"],
    },
    {
      "before": ["leader", "h"],
      "commands": [":split"],
    },
    // Nice to have
    {
      "before": ["leader", "w"],
      "commands": [":w!"],
    },
    {
      "before": ["leader", "q"],
      "commands": [":q!"],
    },
    {
      "before": ["leader", "x"],
      "commands": [":x!"],
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"],
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"],
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"],
    },
    {
      "before": ["leader", "s", "f"],
      "commands": ["workbench.action.quickOpen"],
    },
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"],
    },
  ],
  "vim.visualModeKeyBindings": [
    // Navigation between files
    {
      "before": ["<S-h>"],
      "commands": [":bprevious"],
    },
    {
      "before": ["S-l>"],
      "commands": [":bnext"],
    },
    // Stay in visual mode while indenting
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"],
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"],
    },
    {
      "before": ["tab"],
      "commands": ["editor.action.indentLines"],
    },
    {
      "before": ["<S-tab>"],
      "commands": ["editor.action.outdentLines"],
    },
    // Copy and paste
    {
      "before": ["g", "y"],
      "commands": ["editor.action.clipboardCopyAction"],
    },
    {
      "before": ["g", "p"],
      "commands": ["editor.action.clipboardPasteAction"],
    },
  ],
  "editor.scrollBeyondLastLine": false,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "expand",
